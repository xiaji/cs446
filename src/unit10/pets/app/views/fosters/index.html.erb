<h1>Listing fosters</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @fosters.each do |foster| %>
      <tr>
        <td><%= link_to 'Show', foster %></td>
        <td><%= link_to 'Edit', edit_foster_path(foster) %></td>
        <td><%= link_to 'Destroy', foster, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foster', new_foster_path %>
